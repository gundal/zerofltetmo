if [ -f "/system/xbin/busybox" ]; then BB=/system/xbin/busybox; else BB=/sbin/busybox; fi;
if [ -f "/system/bin/busybox" ]; then BB=/system/bin/busybox; else BB=/sbin/busybox; fi;


case $1 in
fuel-gauge)
P=/sys/devices/battery.54/power_supply/battery/fg_reset_cap;
$BB chmod 666 $P;
  echo 1 > $P
  echo "The fuel gauge chip is reset. Give it some time to normalize the gauge."
  sleep 2;
  ;;
esac
